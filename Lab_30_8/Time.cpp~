#include "Time.h"
Time::Time(){
	hours = 0;
	minutes = 0;
	seconds = 0;
}
Time::~Time(){
}
Time::Time(int h, int m, int s){
	hours = h;
	minutes = m;
	seconds = s;
}
int Time::readH(){
	return hours;
}
int Time::readM(){
	return minutes;
}
int Time::readS(){
	return seconds;
}
void Time::setH(int h){
	hours = h;
}
void Time::setM(int m){
	minutes = m;
}
void Time::setS(int s){
	seconds = s;
}

